{% extends "base.html.twig" %}

{% block body %}
<table id="patients">
    <tr>
        <td>ID</td>
        <td>Ime</td>
        <td>Prezime</td>
        <td>Telefon</td>
    </tr>
    {% for patient in patients %}
    <tr>
        <td>{{ patient.id }}</td>
        <td>{{ patient.name }}</td>
        <td>{{ patient.surname }}</td>
        <td>{{ patient.phone }}</td>
    </tr>
    {% endfor %}
</table>
{% endblock %}